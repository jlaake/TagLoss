\end_layout

\begin_layout Plain Layout

mod3_status=tag_status(4,mod3$results$mat$gamma[1,1,,])
\end_layout

\begin_layout Plain Layout

mod4_status=tag_status(4,mod4$results$mat$gamma[1,1,,])
\end_layout

\begin_layout Plain Layout

mod0_status=tag_status(4,mod0$results$mat$gamma[1,1,,])
\end_layout

\begin_layout Plain Layout

@
\end_layout

\end_inset


\end_layout

